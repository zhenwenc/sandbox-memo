    "@types/react": "17.0.15",
    "@types/react-dom": "17.0.11"
  },
  "prettier": {
    "arrowParens": "avoid",
    "printWidth": 110,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "quoteProps": "consistent"
  },
  "jest": {
    "testEnvironment": "node",
    "preset": "@navch/testing"
