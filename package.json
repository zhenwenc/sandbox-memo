    "bs58": "5.0.0",
    "got": "11.8.2",
    "ioredis": "4.28.0",
    "morgan": "1.10.0",
    "next": "12.2.3",
    "pusher": "5.1.1-beta",
    "pusher-js": "7.3.0",
    "ramda": "0.27.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "tslib": "2.4.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@navch/testing": "0.7.5-next.38",
